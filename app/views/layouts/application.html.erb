                <li><%= link_to 'Tubal Cannulation', laparoscopy_tubal_cannulation_path %></li>
                <li><%= link_to 'Cyst', laparoscopy_cyst_path %></li>
                <li><%= link_to 'Endometriosis', laparoscopy_endometriosis_path %></li>
                <li><%= link_to 'Tuboplasty', laparoscopy_tuboplasty_path %></li>
              </ul>
            </li>
            <li><a href="#">Pregnancy</a>
              <ul class="second-menu">
                <li><%= link_to 'Week By Week', pregnancy_week_by_week_path %></li>
                <li><%= link_to 'Prenatal', pregnancy_prenatal_path %></li>
                <li><%= link_to 'Infertility', pregnancy_infertility_path %></li>
              </ul>
            </li>
            <li><a href="#">Birth & Beyond</a>
              <ul class="second-menu">
                <li><%= link_to 'Natural Birth', "#" %></li>
                <li><%= link_to 'Caesarean', "#" %></li>
                <li><%= link_to 'Childbirth exercise', "#" %></li>
                <li><%= link_to 'New Born', "#" %></li>
                <li><%= link_to 'Postnatal', "#" %></li>
              </ul>
            </li>
            <li><a href="#">Appointments</a>
              <ul class="second-menu">
                <li><%= link_to 'Contact', home_contact_path %></li>
                <li><%= link_to 'Location', home_locate_path %></li>
              </ul>
            </li>
          </ul>
        </div>
        <!--end menu-->

      </div>
      <!--end header-->
      <div id="content_bg"><%= yield %></div>

    </div>
    <div id="footer">
      <div class="left"> Â© Rangnath Hospital </div>
      <div class="right">
      </div>
    </div>
    <!--end footer-->

    <!--end wrapper-->




  </body>
</html>
