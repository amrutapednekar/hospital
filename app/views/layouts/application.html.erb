          <h2 class="hospital_desc">A world to take care of you and miracle inside you.</h2>
        </div>
        <!--end logo-->
        <div id="menu">
          <ul id="navigation">
            <li><a href="/home/index.htm" class="active">Home</a></li>
            <li><a href="/laparoscopy/index.htm">Procedures and surgery</a>
              <ul class="second-menu">
                <li><%= link_to 'Laparoscopy', laparoscopy_index_path %></li>
                <li><%= link_to 'Appendicitis', laparoscopy_appendicitis_path %></li>
                <li><%= link_to 'Hernia', laparoscopy_hernia_path %></li>
